{
  "expo": {
    "name": "WalkToGrave",
    "slug": "walktograve-mobile",
    "version": "1.0.0",
    "newArchEnabled": true,
    "scheme": "walktograve",
    "extra": {
      "eas": {
        "projectId": "fb528a6e-f7c3-4863-a595-49936f438a78"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioPermission": "Allow $(PRODUCT_NAME) to record audio"
        }
      ]
    ],
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.jairamsan.WalkToGrave",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "walktogravemobile.com"
            },
            {
              "scheme": "walktograve"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes."
      },
      "associatedDomains": ["applinks:walktogravemobile.com"]
    }
  }
}